{
  "log_level": "warn",
  "backends": {
    "terminal": {
      "enabled": "on",
      "bg_restore_timeout": 3,
      "bg_needs_input": "3b3000",
      "bg_done": "002b00"
    },
    "sound": {
      "enabled": "off",
      "pack": "default",
      "volume": 0.5,
      "command": "",
      "states": {
        "needs-input": true,
        "done": true
      },
      "overrides": {
        "needs-input": "",
        "done": ""
      }
    },
    "desktop": {
      "enabled": "off",
      "states": {
        "needs-input": true,
        "done": true
      },
      "title_format": "[{agent}] {state}",
      "body_format": "{agent} is {state}"
    },
    "push": {
      "enabled": "off",
      "service": "ntfy",
      "topic": "",
      "server": "https://ntfy.sh",
      "token": "",
      "states": {
        "needs-input": true,
        "done": true
      }
    }
  }
}
